<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Admin/Layout/Layout.css" rel="stylesheet" />
    <link href="~/Content/Admin/Mantenimiento/Roles.css" rel="stylesheet" />
</head>
<body>
    <nav class="navbar-main navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-5">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img class="navbar-brand" src="bower_components/it-mlaf-sass/images/intel-logo-white-100.png" alt="">
                <a class="navbar-brand">SIGELIBMA</a>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-5">
                <ul class="nav navbar-nav navbar-right">
                    <li id="nameandjobtitlebar" class="active"><a class=""><i class="intelicon-user-person-outlined" aria-hidden="true"></i>Stevenag | Admin</a></li>
                    <li ng-if="showlogout" class="active"><a href="" ng-click="logout()">Logout </a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div id="page-main" class="">

        @RenderBody()
        
        @*<footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>*@
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
